alias ssh_musefy="ssh www.musefy.com -l musefyco"

alias mysql=/usr/local/mysql/bin/mysql
alias mysqladmin=/usr/local/mysql/bin/mysqladmin

alias "get-shit-done"="sudo ~/bin/get-shit-done/get-shit-done"

